-- This forward model uploads results from reservoir simulators eclipse or opm
-- to sumo. It is called from the ERT config file as a regular forward model

-- Arguments:
-- CONFIG_PATH: path to fmu config, which needs to be p

STDERR ecl2csv_sumo.stderr
STDOUT ecl2csv_sumo.stdout

EXECUTABLE sim2sumo

ARGLIST <CONFIG_PATH> <SUMO_ENV>

MIN_ARG 0
MAX_ARG 1
ARG_TYPE 0 STRING
ARG_TYPE 1 STRING